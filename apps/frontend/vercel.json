{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build" }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://learnova-srwb.onrender.com/api/:path*"
    },
    {
      "source": "/auth/:path*",
      "destination": "https://learnova-srwb.onrender.com/api/auth/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}